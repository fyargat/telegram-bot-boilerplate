{
  "tabWidth": 2,
  "printWidth": 80,
  "semi": true,
  "arrowParens": "always",
  "endOfLine": "auto",
  "trailingComma": "all",
  "singleQuote": false,
  "bracketSpacing": true,
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@/config/(.*)$",
    "^@/context(.*)$",
    "^@/exceptions(.*)$",
    "^@/types(.*)$",
    "^@/logger(.*)$",
    "^@/constants(.*)$",
    "^@/middleware(.*)$",
    "^@/commands(.*)$",
    "^@/scenes(.*)$",
    "^@/utils(.*)",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
